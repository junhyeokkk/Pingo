<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pingo.mapper.MatchMapper">

    <!-- 매칭 매퍼 테이블 삽입 -->
    <insert id="insertMatchMapper" parameterType="com.pingo.entity.match.MatchMapperEntity">
        INSERT INTO "matchMapper" ("userNo", "matchUserNo", "matchNo")
        VALUES (#{userNo}, #{matchUserNo}, #{matchNo})
    </insert>

</mapper>